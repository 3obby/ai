{
  "workbench.colorTheme": "Dark Orange Flat",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.semi": false,
  "editor.wordWrap": "on",
  "solidity.telemetry": false,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "typescript.tsserver.web.projectWideIntellisense.enabled": false,
  "cursor.general.gitGraphIndexing": "disabled",
  "mcpServers": {
    "pinecone-memory": {
      "command": "npx",
      "args": ["-y", "@pinecone-database/pinecone-mcp-server"],
      "env": {
        "PINECONE_API_KEY": "<YOUR_PINECONE_API_KEY>",
        "PINECONE_INDEX": "agentconsult",
        "PINECONE_ENVIRONMENT": "us-east-1-aws"
      }
    }
  },
  "ai.externalMemory": true
} 